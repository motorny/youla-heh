{
  "manifest_version": 2,
  "background": {
    "scripts": [
      "js/lib/jquery-3.5.1.min.js",
      "js/background.js"
    ],
    "persistent": false
  },

  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "identity",
    "declarativeContent",
    "https://www.googleapis.com/",
    "http://194.67.110.28:8008/*",
    "https://www.instagram.com/*"
  ],

  "content_scripts": [
    {
      "css": [
        "/css/bootstrap.min.css",
        "/css/style.css",
        "/css/modals.css",
        "/css/lib/Chart.min.css"
      ],
      "js": [
        "js/lib/Chart.min.js",
        "js/lib/jquery-3.5.1.min.js",
        "js/on_instagram_content.js"
      ],

      "matches": [
        "*://www.instagram.com/*"
      ]
    }
  ],
  "page_action": {
      "default_popup": "html/popup.html",
      "default_icon": {
        "16": "images/logo.png",
        "32": "images/logo.png",
        "48": "images/logo.png"
      }
    },
  
  "web_accessible_resources": [
    "css/style.css",
    "fonts/Helvetica-Bold.ttf",
    "fonts/dintextpro.ttf",
    "images/logo.png",
    "js/my_file.js"
  ],
  "version": "1.0.0",
  "name": "Yoloco stats",
  "description": "Уникальный плагин, позволяющий оценивать пользователей Instagram при размещении рекламы",
  "icons": {
    "128": "images/logo.png",
    "16": "images/logo.png",
    "48": "images/logo.png"
  },
  "key": ""
}